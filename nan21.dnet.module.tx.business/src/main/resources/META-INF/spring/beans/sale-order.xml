	<bean id="SalesOrder" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.tx.business.ext.sale.service.SalesOrder_Service"/>				

	<bean id="SalesOrderTax" scope="singleton" lazy-init="true"
