<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="PurchaseInvoice" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.tx.business.ext.purchase.service.PurchaseInvoice_Service"/>				

	<bean id="PurchaseInvoiceTax" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.tx.business.impl.purchase.PurchaseInvoiceTax_Service"/>				

	<bean id="PurchaseInvoiceLine" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.tx.business.ext.purchase.service.PurchaseInvoiceLine_Service"/>				

	<bean id="PurchaseInvoiceLineTax" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.tx.business.impl.purchase.PurchaseInvoiceLineTax_Service"/>				
</beans>
